@page
@model VasIgASP.NETLibraryWebApp.Pages.ReportModel

@{
    ViewData["Title"] = "Student Body Statistics";
}

<h2>Gallery Statistics</h2>

<table>
    <tr>
        <th>
            Acquirement Date
        </th>
        <th>
            Books
        </th>
    </tr>

    @foreach (var item in Model.Books)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AcquirementDate)
            </td>
            <td>
                @item.BookCount
            </td>
        </tr>
    }
</table>